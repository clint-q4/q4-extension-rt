{
  "icons": {
    "16": "./gen-assets/icon16.png",
    "48": "./gen-assets/icon48.png",
    "128": "./gen-assets/icon128.png"
  },
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "./gen-assets/icon16.png",
      "48": "./gen-assets/icon48.png"
    },
    "default_popup": "./static/popup/index.html"
  },
  "version": "0.0.0",
  "name": "Q4 extension",
  "description": "A basic Plasmo extension.",
  "author": "clintonsebastian",
  "permissions": [
    "storage",
    "storage",
    "activeTab",
    "scripting"
  ],
  "options_ui": {
    "page": "./static/options/index.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "../content.ts"
      ]
    }
  ],
  "host_permissions": [
    "https://*/*"
  ]
}
